package tutorial_2;

public class application {

	public static void main(String[] args) {
		int myNumber_1;
		int myNumber_2 = 69;
		
		short myShort = 11;
		long myLong = 100000;
		double myDouble = 3.14;
		float myFloat = 69.69f; //add f at end to make float
		char myChar = 'A';
		boolean myBoolean = true;
		byte myByte = 127;
		
		
		myNumber_1 = 88;
		
		System.out.println(myNumber_1);
		System.out.println("The value of myNumber_2 is: " + myNumber_2);
		System.out.println("The value of myShort is: " + myShort);
		
	}

}
